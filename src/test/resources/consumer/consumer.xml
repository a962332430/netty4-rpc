<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">


    <bean id="userService" class="com.netty4.rpc.client.factory.spring.ServiceSpringConsumerBean">
        <property name="interfaceName" value="com.netty4.rpc.demo.api.UserService" />
    </bean>

    <bean id="orderService" class="com.netty4.rpc.client.factory.spring.ServiceSpringConsumerBean">
        <property name="interfaceName" value="com.netty4.rpc.demo.api.OrderService" />
    </bean>

</beans>
